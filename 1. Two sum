class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int,int>mp;
        for(int i=0;i<nums.size;i++){
            int luck = target - nums[i];
            if (mp.find(target)!=mp.end()){
                return {mp[target],i};
            }
            mp[nums[i]]=i;
        }
        return{};
    }
};
